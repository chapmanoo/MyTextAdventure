package main;
import java.util.LinkedList;

public class ItemManager {
	
	private LinkedList<Item> itemList = new LinkedList<Item>();
	
	public boolean addItem(Item i) {
		try {
			itemList.add(i);
			return true;
		} catch (Exception e)
		{
			return false;
		}
	}
	
	public LinkedList<Item> getItemList()
	{
		return itemList;
	}
	
	

}
